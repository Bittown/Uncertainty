<div class="container">
  <div class="jumbotron">
    <%= render 'share/simple_search', url: users_path %>
  </div>

  <div class="panel panel-default" %>
    <div class="panel-heading">
      <p class="panel-title pull-left">Users</p>
      <div class="clearfix"></div>
    </div>

    <div class="panel-body">
      <%= will_paginate %>
    </div>

    <div class="list-group">
      <%= render(@users) || t('no_users') %>
    </div>
  </div>

</div>
